@using Microsoft.AspNet.Identity
<!DOCTYPE html>
<html lang="en">


<!-- choosyfoody/index.html  29 Nov 2019 03:38:14 GMT -->
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <title>ChoosyFoddy | HTML Restaurant Theme</title>

    <!-- GOOGLE FONTS CSS -->
    <link href="https://fonts.googleapis.com/css?family=Great+Vibes|Poppins:400,500,600,700,800&amp;display=swap"
          rel="stylesheet">

    <!-- FONTAWESOME CSS -->
    <link rel="stylesheet" href="~/Content/css/all.min.css">

    <!-- BOOTSTRAP CSS -->
    <link rel="stylesheet" href="~/Content/css/vendors/bootstrap.min.css">

    <!-- SLICK FOR SLIDER CSS -->
    <link rel="stylesheet" href="~/Content/css/vendors/slick.css">
    <link rel="stylesheet" href="~/Content/css/vendors/slick-theme.css">

    <!-- BOOTSTRAP DATETIME PICKER CSS -->
    <link href="~/Content/datetimepicker/css/tempusdominus-bootstrap-4.css" rel="stylesheet">

    <!-- MAIN CSS -->
    <link rel="stylesheet" href="~/Content/css/main/style.css">

    <!-- RESPONSIVE CSS -->
    <link rel="stylesheet" href="~/Content/css/responsive/responsive.css">

</head>

<body>

    <!-- NAV BAR PART START -->
    <nav id="navbar-id" class="navbar navbar-expand-lg navbar-fixed-top navbar-light cf-nav">
        <div class="container">

            <a id="logo" class="navbar-brand" href="index.html">
                <img src="~/Content/images/cf-logo.png" alt="logo" class="img-fluid">
            </a>

            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent"
                    aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav ml-auto">
                    <li class="nav-item">
                        <a class="nav-link" href="@Url.Action("Index","Home")">Home <span class="sr-only">(current)</span></a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="@Url.Action("About","Home")">About Us</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="@Url.Action("Menu","Home")">Menu</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="@Url.Action("OurChefs","Home")">Our Chefs</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="contact-us.html">Contact Us</a>
                    </li>

                    @Html.Partial("_LoginPartial")
                    <li class="nav-item">
                        <a href="#anchor-booktable">
                            <bottom type="button" class="btn all-btn shine">Book-a-Table</bottom>
                        </a>
                    </li>
                </ul>
            </div>

        </div>
    </nav>
    <!-- NAV BAR PART END -->
    @RenderBody()
    <!-- FOOTER PART START -->
    <section id="full-footer" class="footer">

        <div class="footer-top">

            <div class="container">

                <div class="row">

                    <div class="col col-12 col-sm-6 col-md-6 col-xl-4 col-1">

                        <div class="row">

                            <div class="col-md-12 logo">
                                <p>
                                    <a href="index.html"><img src="~/Content/images/cf-logo.png" alt="logo"></a>
                                </p>
                            </div>

                            <div class="col-md-12">

                                <div class="social">
                                    <p class="text">Around The Web</p>
                                    <ul>
                                        <li>
                                            <ul>
                                                <li>
                                                    <a class="facebook" href="YOUR%20FACEBOOK%20URL.html">
                                                        <i class="fab fa-facebook-f"></i>
                                                    </a>
                                                </li>
                                                <li>
                                                    <a class="twitter" href="YOUR%20TWITTER%20URL.html">
                                                        <i class="fab fa-twitter"></i>
                                                    </a>
                                                </li>
                                                <li>
                                                    <a class="instagram" href="YOUR%20INSTAGRAM%20URL.html">
                                                        <i class="fab fa-instagram"></i>
                                                    </a>
                                                </li>
                                                <li>
                                                    <a class="behance" href="YOUR%20BEHANCE%20URL.html">
                                                        <i class="fab fa-behance"></i>
                                                    </a>
                                                </li>
                                                <li>
                                                    <a class="linkedin" href="YOUR%20LINKED%20IN%20URL.html">
                                                        <i class="fab fa-linkedin-in"></i>
                                                    </a>
                                                </li>
                                                <li>
                                                    <a class="youtube" href="YOUR%20YOUTUBE%20URL.html">
                                                        <i class="fab fa-youtube"></i>
                                                    </a>
                                                </li>
                                            </ul>
                                        </li>
                                    </ul>

                                </div>
                            </div>
                        </div>

                    </div>

                    <div class="col col-12 col-sm-6 col-md-6 col-xl-1 col-2">
                        <ul>
                            <li><a href="index.html">Home</a></li>
                            <li><a href="about-us.html">About Us</a></li>
                            <li><a href="menu.html">Menu</a></li>
                            <li><a href="our-chefs.html">Our Chefs</a></li>
                            <li><a href="contact-us.html">Contact</a></li>
                        </ul>
                    </div>

                    <div class="col col-12 col-sm-6 col-md-6 col-xl-4 col-3">

                        <div class="phone">
                            <p>+1 234 567 890</p>
                        </div>

                        <div class="mail">
                            <p>INFO@CHOOSYFOODY.COM</p>
                        </div>

                        <div class="address">
                            <p>77 GROVE STREET</p>
                            <p>SOUTH NEWYORK</p>
                            <p>NEWYORK 10013</p>
                            <p>UNITED STATES</p>
                        </div>

                    </div>

                    <div class="col col-12 col-sm-6 col-md-6 col-xl-3 col-4">

                        <p>Write something to us</p>

                        <form action="#">
                            <div class="input-group">
                                <input type="name" class="form-control" placeholder="Your name" autocomplete="off">
                            </div>

                            <div class="input-group">
                                <input type="email" class="form-control" placeholder="Your email" autocomplete="off">
                            </div>

                            <div class="input-group">
                                <textarea class="form-control" name="text" id="text" placeholder="Your message"
                                          autocomplete="off"></textarea>
                            </div>

                            <div class="input-group">
                                <span class="input-group-btn">
                                    <button class="btn" type="submit">Submit</button>
                                </span>
                            </div>
                        </form>

                    </div>

                </div>

            </div>
        </div>
        <div class="footer-bottom">
            <div class="container">

                <div class="row">

                    <div class="col-md-8 col-left">

                        <a href="templateshub.net">Templates Hub</a>

                    </div>

                    <div class="col-md-4 col-right d-none d-md-block">

                    </div>

                </div>

            </div>
        </div>

    </section>
    <!-- FOOTER PART END -->
    <!-- CORE JQUERY -->
    <script src="~/Content/js/vendors/jquery-1.12.4.min.js"></script>

    <!-- BOOTSTRAP JS -->
    <script src="~/Content/js/vendors/popper.min.js"></script>
    <script src="~/Content/js/vendors/bootstrap.min.js"></script>

    <!-- SLIDERS JS -->
    <script src="~/Content/js/vendors/slick.min.js"></script>

    <!-- FILTER USED IN SPECIAL PART -->
    <script src="~/Content/js/vendors/isotope.pkgd.min.js"></script>

    <!-- PARALLAX USED IN SPECIAL PART-->
    <script src="~/Content/js/vendors/rellax.min.js"></script>

    <!-- DATE TIME PICKER JS Start-->
    <script src="~/Content/js/moment-with-locales.min.js"></script>
    <script src="~/Content/datetimepicker/js/tempusdominus-bootstrap-4.js"></script>

    <!-- CUSTOM JS -->
    <script src="~/Content/js/main/custom.js"></script>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>


